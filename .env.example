# Example environment file for fdic-bank-find-mcp-server
# Copy to .env and edit as needed

# Set RUST_LOG to debug for verbose logging
RUST_LOG=info
# Maximum allowed FDIC API response body size in bytes (default: 1073741824 for 10MB)
FDIC_MAX_RESPONSE_CONTENT_LENGTH=1073741824
# Log directory (default: "logs")
FDIC_MCP_LOG_DIR=target/debug/logs
# Comma-separated list of endpoints to exclude from the server
ENDPOINT_EXCLUSIONS=financials
