# Example environment file for fdic-bank-find-mcp-server
# Copy to .env and edit as needed

# Bind address for MCP server (default: 127.0.0.1)
BIND_ADDR=127.0.0.1
# Port to bind to (default: 5000)
BIND_PORT=5000
# Set RUST_LOG to debug for verbose logging
RUST_LOG=debug
# Maximum allowed FDIC API response body size in bytes (default: 5242880 for 5MB)
FDIC_MAX_RESPONSE_CONTENT_LENGTH=5242880